    {% if breadcrumbs is not empty %}
        <nav aria-label="Breadcrumb" class="sm:flex">
            <ol role="list" class="flex items-center">
                {% for index, breadcrumb in breadcrumbs %}
                    <li>
                        <div class="flex items-center">
                            {% if index is not same as 0 %}
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true" data-slot="icon" class="text-gray-400 shrink-0 size-5">
                                    <path fill-rule="evenodd" d="M8.22 5.22a.75.75 0 0 1 1.06 0l4.25 4.25a.75.75 0 0 1 0 1.06l-4.25 4.25a.75.75 0 0 1-1.06-1.06L11.94 10 8.22 6.28a.75.75 0 0 1 0-1.06Z" clip-rule="evenodd"></path>
                                </svg>
                            {% endif %}
                            <a href="{{ breadcrumb.url }}" class="text-gray-400 font-medium text-sm">
                                {{ breadcrumb.label|trans }}
                            </a>
                        </div>
                    </li>
                {% endfor %}
            </ol>
        </nav>
    {% endif %}
</div>